---
import ResumeElement from '$components/resume/containers/ResumeElement.astro';
import ResumeParagraphLine from '$components/resume/containers/ResumeParagraphLine.astro';
import School from 'svelte-ionicons/School.svelte';
---

<ResumeElement title="Formation" mb>
    <ResumeParagraphLine title="CESI École d'Ingénieurs" subtitle="2021 - 2026, Nancy" icon={School}>
        Actuellement en première année de cycle ingénieur informatique
    </ResumeParagraphLine>
    <ResumeParagraphLine title="Lycée Charles Jully" subtitle="2018 - 2021, Saint-Avold" icon={School}>
        Baccalauréat général mention bien avec les spécialités mathématiques, physique-chimie et informatique
    </ResumeParagraphLine>
</ResumeElement>
