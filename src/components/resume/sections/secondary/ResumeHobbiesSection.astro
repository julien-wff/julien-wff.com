---
import ResumeElement from '../../containers/ResumeElement.astro';
import ResumeIconLine from '../../containers/ResumeIconLine.astro';
import Barbell from 'svelte-ionicons/Barbell.svelte';
import Code from 'svelte-ionicons/Code.svelte';
import Body from 'svelte-ionicons/Body.svelte';
---

<ResumeElement title="Passions" gap="small">
    <ResumeIconLine icon={Barbell}>
        Sport
    </ResumeIconLine>
    <ResumeIconLine icon={Code}>
        Informatique
    </ResumeIconLine>
    <ResumeIconLine icon={Body}>
        Théâtre (4 ans)
    </ResumeIconLine>
</ResumeElement>
