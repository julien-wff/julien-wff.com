{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "$pages/*": [
        "src/pages/*"
      ],
      "$components/*": [
        "src/components/*"
      ],
      "$layouts/*": [
        "src/layouts/*"
      ],
      "$assets/*": [
        "src/assets/*"
      ],
      "$i18n/*": [
        "src/i18n/*"
      ]
    }
  }
}